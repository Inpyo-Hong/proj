<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cookandroid.newproj">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <uses-sdk />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NewProj">
        <activity android:name=".Today_Exercise.Camera.CameraActivity"></activity>
        <activity android:name=".Exercise.Exercise_TIP.exercise_tip" />
        <activity android:name=".Exercise.Exercise_Cycle.routine" />
        <activity android:name=".Today_Exercise.today_exercise" />
        <activity android:name=".Exercise.exercise" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Exercise.Exercise_Cycle.cycle.cycle_a"/>
        <activity android:name=".Exercise.Exercise_Cycle.cycle.cycle_b"/>
        <activity android:name=".Exercise.Exercise_Cycle.cycle.cycle_c"/>
        <activity android:name=".Exercise.Exercise_TIP.Tip.tip_squat"/>
        <activity android:name=".Exercise.Exercise_TIP.Tip.tip_pullup"/>
        <activity android:name=".Exercise.Exercise_TIP.Tip.tip_pushup"/>
    </application>

</manifest>