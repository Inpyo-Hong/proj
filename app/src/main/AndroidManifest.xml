<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.iip">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="PAT"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Iip">
        <activity android:name="com.project.iip.activity.Camera.CameraActivity"></activity>
        <activity android:name="com.project.iip.activity.TodayExerciseActivity"></activity>
        <activity android:name="com.project.iip.activity.cycle_a"></activity>
        <activity android:name="com.project.iip.activity.cycle_b"></activity>
        <activity android:name="com.project.iip.activity.cycle_c"></activity>
        <activity android:name="com.project.iip.activity.cycle_d"></activity>
        <activity android:name="com.project.iip.activity.ExerciseInformationActivity" />
        <activity android:name="com.project.iip.activity.TipActivity" />
        <activity android:name="com.project.iip.activity.RoutineActivity" />
        <activity android:name="com.project.iip.activity.ExerciseActivity" />
        <activity android:name="com.project.iip.activity.MainActivity"/>
        <activity android:name="com.project.iip.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.project.iip.activity.Exercise_TIP.Tip.tip_pullup"/>
        <activity android:name="com.project.iip.activity.Exercise_TIP.Tip.tip_pushup"/>
        <activity android:name="com.project.iip.activity.Exercise_TIP.Tip.tip_squat"/>
    </application>

</manifest>